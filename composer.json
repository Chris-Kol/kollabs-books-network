{
    "name": "chris-kol/kollabs-books-network",
    "description": "A dummy book catalog application",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ramsey/uuid": "^4.2",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5",
        "php-di/php-di": "^6.3",
        "aura/sql": "^5.0",
      "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "KollabsBooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KollabsBooks\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}